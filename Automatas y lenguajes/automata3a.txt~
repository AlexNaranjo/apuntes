digraph finite_state_machine {
    rankdir=LR;
    size="8,5"

    node [shape = doublecircle]; q5;
    node [shape = point ]; init

    node [shape = circle];
    init -> q0
    q0	 -> q1 [ label = "Î»" ];
    q1    -> q2  [ label = "1" ];
    q2   -> q3 [ label = "1" ];
    q2 -> q4 [ label = "0" ];
    q3 -> q5 [ label = "0" ];
    q4 -> q5 [ label = "1" ];
}
