digraph automata_5 {
    rankdir=LR;
    size="8,5"

    node [shape = doublecircle]; f;
    node [shape = point ]; init
    node [shape = circle];

     init -> p;
     p -> p [label = "{z,1,az}{a,1,aa}{b,1,ab}{z,0,bz}{a,0,ba}{b,0,bb}"];
     p -> q [label = "{a,1,λ}{b,0,λ}{z,λ,z}"];
     q -> q [label = "{a,1,λ}{b,0,λ}"];
     f -> f [label = "{z,λ,z}"];
}
