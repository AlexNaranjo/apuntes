digraph automara_2a {
    rankdir=LR;
    size="8,5"

    node [shape = doublecircle]; Qfin;
    node [shape = point ]; init
    node [shape = circle];

     init -> Qini;
     Qini -> Q1 [label = "@"];
     Q1 -> Q2 [label = "a,b,...,z"];
     Q2 -> Qfin [label = "0,1,...,9,a,b,...,z"];
     Qfin -> Qfin [label = "0,1,...,9,a,b,...,z"];
}
