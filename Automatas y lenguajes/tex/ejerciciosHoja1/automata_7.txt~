digraph automara_5 {
    rankdir=LR;
    size="8,5"

    node [shape = doublecircle]; f;
    node [shape = point ]; init
    node [shape = circle];

     init -> p;
     p -> p [label = "{z,x,xz}{y,x,xy}{x,x,xx}"];
     p -> q [label = "{x,x,λ}"];
     q -> q [label = "{x,x,λ}"];
     q -> f [label = "{z,λ,z}"];
}
